{
  "name": "rackles.io",
  "description": "Personal website of John Rackles",
  "engines": {
    "node": ">=15.0.0"
  },
  "license": "GPL-3.0-or-later",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "vite-node ./src/generate-pdf.tsx",
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/cloudflare-pages/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "wrangler pages publish ./dist",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "serve": "wrangler pages dev ./dist",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "prepare": "husky"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.1.5",
    "@builder.io/qwik-city": "^1.1.5",
    "@tailwindcss/typography": "^0.5.9",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.13.4",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "autoprefixer": "^10.4.19",
    "daisyui": "^5.0.0-beta.8",
    "eslint": "^8.49.0",
    "eslint-plugin-qwik": "^1.1.5",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "postcss": "^8.4.39",
    "prettier": "^3.5.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.7",
    "typescript": "^5.2.2",
    "undici": "^7.3.0",
    "vite": "^6.1.0",
    "vite-node": "^3.0.6",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^3.0.0"
  },
  "dependencies": {
    "@builder.io/qwik-react": "^0.5.0",
    "@fontsource-variable/recursive": "^5.0.3",
    "@react-pdf/renderer": "^4.2.2",
    "@tailwindcss/vite": "^4.0.7",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.9",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "volta": {
    "node": "22"
  },
  "lint-staged": {
    "!(*.ts)": "node --experimental-strip-types node_modules/prettier/bin/prettier.cjs . --write",
    "*.ts": [
      "eslint --fix",
      "node --experimental-strip-types node_modules/prettier/bin/prettier.cjs . --write"
    ]
  }
}
